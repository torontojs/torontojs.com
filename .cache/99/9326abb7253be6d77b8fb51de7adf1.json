e(`events`, `addEvent`, []),\n  lifecycle({\n    componentDidMount() {\n      Object.values(eventUrls).map(async val => {\n        let r = await fetch(val, { mode: `cors` })\n        let { items } = await r.json()\n        this.props.addEvent(s => [\n          ...s,\n          ...(items || []).filter(event =>\n            event.organizer && +Date.now() < +new Date(event.end.dateTime)\n          ),\n        ].sort((a, b) => +new Date(a.start.dateTime) - +new Date(b.start.dateTime)))\n      })\n    },\n  })\n)(({ events }) => (\n  <div>\n    <SectionTitle>\n      UPCOMING EVENTS\n    </SectionTitle>\n    <Card>\n      {events.map((e, i) =>\n        <div key={e.id}>\n          <Event event={e} />\n          {i < events.length - 1 && <hr />}\n        </div>\n      )}\n    </Card>\n  </div>\n))\n"},"lineCount":null}},"error":null,"hash":"798828d74f76cdea08b98f7f49745d8f","cacheData":{"env":{}}}