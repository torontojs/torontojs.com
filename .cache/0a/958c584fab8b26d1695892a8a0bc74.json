;\n  text-decoration: none;\n  color: #333;\n  display: flex;\n  align-items: center;\n`\n\nconst StaticPage = props => {\n  const [content, setContent] = useState(``)\n\n  useEffect(() => {\n    async function fetchPage() {\n      let data = await fetch(`${basePath}/pages/${props.match.params.page}.md`)\n        .then((response) => response.text())\n\n      if (data.match(/<head>/g)) {\n        this.props.history.replace({ pathname: `/404` })\n      } else {\n        setContent(data)\n      }\n    }\n\n    fetchPage()\n  })\n\n  return (\n    <div>\n      <div>\n        <Link to=\"/\">\n          <Logo src={logo} />\n          <span>BACK</span>\n        </Link>\n      </div>\n      <Page>\n        <ReactMarkdown source={content} />\n      </Page>\n    </div>\n  )\n}\n\nexport default StaticPage"},"lineCount":null}},"error":null,"hash":"4407df4091c876a43b99d5f9d8c1d367","cacheData":{"env":{"NODE_ENV":"development"}}}